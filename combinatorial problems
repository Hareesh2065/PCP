public class Main{
    public static void main(String[] args){
        int c = 10;
        for (int i = 1; i <= 6; i++) {  // Runs 6 times (i = 1 to 6)
            for (int j = 1; j <= i; j++) {  // Runs i times for each i
                c = c + 3;  // Increases `c` by 3 each iteration
            }
        }
        System.out.println(c);
    }
}
